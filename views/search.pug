extends layouts/main
include mixins/videoBlock

block content
    .search__header
        h3 Search for : #{searchingBy}
    .search__videos
        if videos.length === 0
            h2 No Video Found
        each item in videos
            +videoBlock( {
                id : item.id,
                title: item.title,
                views: item.description,
                videoFile: item.fileUrl
            })